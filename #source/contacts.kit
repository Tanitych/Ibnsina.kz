<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Контакты</title>
    <link rel="shortcut icon" href="img/header-img/icons/favicon.png" />
    <link rel="stylesheet" href="css/bootstrap.min.css" />
    <link rel="stylesheet" href="lips/fontawesome/all.min.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />

    <link rel="stylesheet" href="css/slick.css" />
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <!-- @import "header.html" -->

    <div class="contacts">
      <div class="content">
        <div class="contacts__block">
          <div class="contacts__block__left">
            <h3 class="contacts__block__title">Контакты</h3>
            <div class="contacts__block__left__items">
              <div class="img">
                <img src="img/contacts/icons/tel.svg" alt="" />
              </div>
              <div class="text">
                <p>Телефон</p>
                <span>+7 (700) 500 - 50-50</span>
              </div>
            </div>

            <div class="contacts__block__left__items">
              <div class="img">
                <img src="img/contacts/icons/loc.svg" alt="" />
              </div>
              <div class="text">
                <p>Адрес</p>
                <span>г.Алматы, ул. Сатпаева, 52</span>
              </div>
            </div>
            <div class="contacts__block__left__items">
              <div class="img">
                <img src="img/contacts/icons/mail.svg" alt="" />
              </div>
              <div class="text">
                <p>Почта</p>
                <span>ibnsina.kz@gmail.com</span>
              </div>
            </div>
          </div>
          <div class="contacts__block__right">
            <div class="map" id="map" style="width: 500px; height: 400px"></div>
          </div>
        </div>
      </div>
    </div>

    <!-- @import "footer.html" -->

    <script src="js/jq/jquery.js"></script>
    <script src="js/popper.min.js "></script>
    <script src="js/bootstrap.min.js "></script>
    <script src="js/slick.min.js"></script>
    <script src="https://maps.api.2gis.ru/2.0/loader.js?pkg=full"></script>
    <script src="js/script.js"></script>
  </body>
